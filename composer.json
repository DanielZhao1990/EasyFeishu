{
  "name": "danielzhao1990/easy_feishu",
  "description": "飞书SDK",
  "keywords": [
    "feishu",
    "larksuite",
    "feishu-sdk",
    "larksuite-sdk"
  ],
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "zwf",
      "email": "365333154@qq.com"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "guzzlehttp/guzzle": "^6.3",
    "pimple/pimple": "~3.0",
    "psr/simple-cache": "^1.0",
    "symfony/http-foundation": "^2.7 || ^3.0 || ^4.0 || ^5.0",
    "symfony/event-dispatcher": "^4.3 || ^5.0",
    "monolog/monolog": "^1.22 || ^2.0",
    "symfony/cache": "^3.3 || ^4.3 || ^5.0"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "EasyFeishu\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "EasyFeishu\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5.23"
  }
}
